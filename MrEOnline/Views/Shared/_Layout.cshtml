<!DOCTYPE HTML>

<html>
<head>
    <title>Mr. E Online Rental</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/main.css" rel="stylesheet" />
</head>

<body>

    <!-- Header -->
    <header id="header" class="alt">
        <div class="logo">@Html.ActionLink("Mr. E Online Rental", "Index", "Home")</div>
        <a href="#menu" class="toggle"><span>Menu</span></a>
    </header>

    <!-- Nav -->
    <nav id="menu">

        <ul class="links">
            <li><i class="icon fa fa-user"></i>@Html.Label((string)ViewBag.RemoveSelectedTitle, (string)ViewBag.RemoveSelectedactionName, new { @class = "fa fa-user" })</li>

            <li>@Html.ActionLink("Home", "Index", "Home")</li>
            @*<li><a href="generic.html">Generic</a></li>
                <li><a href="elements.html">Elements</a></li>*@
        </ul>
    </nav>

    @RenderBody()

    <!-- Footer -->
    <footer id="footer" class="wrapper">
        <div class="inner">
            <div class="copyright">
                &copy; Mr. E Online Rental.
            </div>
        </div>
    </footer>
    <!-- Scripts -->
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/jquery.scrolly.min.js"></script>
    <script src="~/Scripts/jquery.scrollex.min.js"></script>
    <script src="~/Scripts/skel.min.js"></script>
    <script src="~/Scripts/util.js"></script>
    <script src="~/Scripts/main.js"></script>
    <script src="~/Scripts/loadash.js"></script>
    <script src="~/Scripts/sweetalert2.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    @RenderSection("scripts", required: false)
    @RenderSection("style", required:false)
</body>
</html>

